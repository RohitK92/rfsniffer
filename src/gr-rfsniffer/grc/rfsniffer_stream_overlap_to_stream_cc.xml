<?xml version="1.0"?>
<block>
  <name>Stream w/ Overlap to Stream (Complex->Complex)</name>
  <key>rfsniffer_stream_overlap_to_stream_cc</key>
  <category>RF Sniffer</category>
  <import>import rfsniffer</import>
  <make>rfsniffer.stream_overlap_to_stream_cc($nitems_per_block, $overlap)</make>
   <param>
    <name>Items per block</name>
    <key>nitems_per_block</key>
    <type>int</type>
  </param>
  <param>
    <name>Overlap</name>
    <key>overlap</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
